net: "models/epic_kitchens_val_rgb.prototxt"
test_iter: 383 # how many mini-batches to test in each validation phase
test_interval: 250 # how often do we call the test phase
test_initialization: true
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 499
display: 20 # how often do we print training loss
average_loss: 20
max_iter: 1000 
iter_size: 1
momentum: 0.9
weight_decay: 0.0005
clip_gradients: 40
snapshot: 400
snapshot_prefix: "models/epic_kitchens_model"
