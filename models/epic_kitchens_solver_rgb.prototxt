net: "models/epic_kitchens_val_rgb.prototxt"
test_iter: 100 # how many mini-batches to test in each validation phase
test_interval: 15 # how often do we call the test phase
base_lr: 0.01
lr_policy: "step"
gamma: 0.1
stepsize: 100
display: 1 # how often do we print training loss
max_iter: 1000 
iter_size: 1
momentum: 0.9
weight_decay: 0.0005
clip_gradients: 40
snapshot: 50
snapshot_prefix: "models/epic_kitchens_model"
