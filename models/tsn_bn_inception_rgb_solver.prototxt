net: "models/tsn_bn_inception_rgb_train_val.prototxt"

# testing parameter
test_iter:100
test_interval: 20
test_initialization: true

# output
display: 10
average_loss: 10
#snapshot: 500
snapshot_prefix: "models/split1_tsn_rgb_bn_inception"
debug_info: false

# learning rate
base_lr: 0.001
lr_policy: "step"
gamma: 0.1
stepsize: 1500
max_iter: 3500
iter_size: 1

# parameter of SGD
momentum: 0.9
weight_decay: 0.0005
clip_gradients: 40

# GPU setting
solver_mode: CPU

